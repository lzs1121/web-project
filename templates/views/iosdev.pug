	section.jr-instructors
		.contain
			h2.text-center Learn with the Best
			+teachers
	section.jr-reviews
		.contain
			header
				h2.blue.inline Student Reviews
			.jr-reviews__container.row
				.col-sm-4
					.jr-reviews__card.stats
						.stats__average
							span 4.9
							.stats__container
								.jr-stars
									div(style="width:95%;")
						.jr-reviews__overview
							.wrapper
								.h6.text-right 5 stars
								.progress
									div(style="width:87%;") 62
							.wrapper
								.h6.text-right 4 stars
								.progress
									div(style="width:13%;") 6
							.wrapper
								.h6.text-right 3 stars
								.progress
									div(style="width:0%;") 0
							.wrapper
								.h6.text-right 2 stars
								.progress
									div(style="width:0%;") 0
							.wrapper
								.h6.text-right 1 stars
								.progress
									div(style="width:0%;") 0
